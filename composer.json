{
    "name": "escolalms/pencil-spaces",
    "description": "Escola Headless LMS Pencil Spaces integration",
    "type": "package",
    "require": {
        "php": ">=8.0",
        "escolalms/core": "^1",
        "escolalms/settings": "^0",
        "laravel/framework": "^8|^9|^10"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": ">=5.0",
        "nunomaduro/larastan": "^1.0",
        "escolalms/auth": "^0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Tomasz Smolarek",
            "email": "tomasz.smolarek@escolasoft.com"
        },
        {
            "name": "Maciej Rymarz",
            "email": "maciej.rymarz@escolasoft.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "EscolaLms\\PencilSpaces\\": "src",
            "EscolaLms\\PencilSpaces\\Tests\\": "tests",
            "EscolaLms\\PencilSpaces\\Database\\Factories\\": "database/factories",
            "EscolaLms\\PencilSpaces\\Database\\Seeders\\": "database/seeders"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EscolaLms\\PencilSpaces\\EscolaLmsPencilSpacesServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": false,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
